// Archmage.cs
using System;

namespace MagicalInheritance
{
 class Archmage : Mage
 {
   public Archmage(string title) : base(title)
   {
   }

   public override Storm CastRainStorm()
   {
     Storm rainSpell = new Storm("rain", true, Title);
     return rainSpell;
   }

    public Storm CastLightningStorm()
    {
     Storm lightSpell = new Storm("lightning", true, Title);
     return lightSpell;
    }


 }
}
